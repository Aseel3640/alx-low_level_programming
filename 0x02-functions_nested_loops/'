#include <stdio.h>
#include <ctype.h>
/**
 *main - Entry point
 *print_sign - prints the sign og number
 *Return: 1 or 0 or -1
 */
int main(void)
{
int print_sign(int n)
{
if (n > 0)
{
putchar('+');
return (1);
}
else if (n == 0)
{
putchar('0');
return (0);
}
else
{
putchar('-');
return (-1);
}
}
return (0);
}
